{
  "name": "convert-sh-theme",
  "version": "0.1.1",
  "description": "Convert VSCode Themes to Shiki/Code Hike css variables",
  "main": "dist/index.js",
  "bin": {
    "convert-sh-theme": "dist/index.js"
  },
  "files": [
    "dist/*",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "shiki", "VSCode", "CodeHike", "theme"
  ],
  "author": "Sebastian Sdorra <s.sdorra@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@vitest/coverage-c8": "^0.27.1",
    "prettier": "^2.8.3",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.27.1"
  },
  "dependencies": {
    "cmd-ts": "^0.11.0",
    "node-fetch-cjs": "^3.2.0",
    "shiki": "^0.12.1"
  }
}
